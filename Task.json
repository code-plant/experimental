{
  "install": {
    "cmd": "yarn install"
  },
  "build": {
    "cmd": "yarn workspace @this-project/development-scripts-task start -C ../../../.. -f task/build.json -k build"
  },
  "test": {
    "cmd": "sh ./scripts/for-each-package.sh npm test"
  },
  "all": {
    "cmd": "yarn install && yarn task build && yarn task test"
  }
}
